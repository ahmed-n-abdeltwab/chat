{
  "name": "chat",
  "version": "1.0.0",
  "description": "chat app",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\" \"public/**/*.{js,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\" \"public/**/*.{js,html,css}\"",
    "format:public": "prettier --write \"public/**/*.{js,html,css}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmed-n-abdeltwab/chat.git"
  },
  "author": "ahmed nasser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahmed-n-abdeltwab/chat/issues"
  },
  "homepage": "https://github.com/ahmed-n-abdeltwab/chat#readme",
  "dependencies": {
    "express": "^4.18.2",
    "lokijs": "^1.5.12",
    "nedb": "^1.8.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/lokijs": "^1.5.14",
    "@types/nedb": "^1.8.13",
    "@types/node": "^20.4.5",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.6.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
